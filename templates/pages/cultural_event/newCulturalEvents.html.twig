{% extends 'base.html.twig' %}

{% block title %}<i class="fa-solid fa-icons"></i> Billet d'événement{% endblock %}

{% block body %}
    {% for label, messages in app.flashes %}
        <div class="alert alert-{{ label }}">
            {% for message in messages %}
                {{ message }}
            {% endfor %}
        </div>
    {% endfor %}
    

{% endblock %}

