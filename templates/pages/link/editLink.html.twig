{% extends 'base.html.twig' %}

{% block title %} Lien{% endblock %}

{% block body %}
    <h5>Modifier le lien :</h5>
    <a href="{{ path('app_home') }}"><i class="fa-solid fa-arrow-left-long"></i> Quitter sans modifier</a>
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.url) }}
    <button type="submit">Enregistrer</button>
    {{ form_end(form) }}
{% endblock %}